{
  "badge": "\u0935\u0921\u094b\u0926\u0930\u093e \u0915\u0940 \u0917\u0948\u0930-\u0932\u093e\u092d\u0915\u093e\u0930\u0940 \u0938\u0902\u0938\u094d\u0925\u093e",
  "headline": "\u091c\u0940\u0935\u0928\u094b\u0902 \u0915\u094b \u091c\u094b\u0921\u093c\u0928\u093e\u0964 \u0938\u094d\u0935\u093e\u0938\u094d\u0925\u094d\u092f \u0915\u094b \u0938\u0936\u0915\u094d\u0924 \u092c\u0928\u093e\u0928\u093e\u0964",
  "lead": "\u0939\u092e \u0930\u0915\u094d\u0924 \u0938\u092e\u0942\u0939 \u091c\u093e\u0917\u0930\u0942\u0915\u0924\u093e \u0914\u0930 \u0938\u094d\u092e\u093e\u0930\u094d\u091f \u0915\u093e\u0930\u094d\u0921, \u092f\u0941\u0935\u093e \u0905\u092d\u093f\u092f\u093e\u0928, \u0906\u092f\u0941\u0937\u094d\u092e\u093e\u0928 \u092f\u094b\u091c\u0928\u093e \u092e\u093e\u0930\u094d\u0917\u0926\u0930\u094d\u0936\u0928, \u091c\u093c\u0930\u0942\u0930\u0924\u092e\u0902\u0926\u094b\u0902 \u0915\u0947 \u0932\u093f\u090f \u092e\u093e\u0938\u093f\u0915 \u0926\u0935\u093e\u0907\u092f\u093e\u0901, \u0914\u0930 \u0935\u0902\u091a\u093f\u0924 \u092a\u0930\u093f\u0935\u093e\u0930\u094b\u0902 \u0915\u0947 \u0932\u093f\u090f \u092c\u0940\u092e\u093e \u0938\u0939\u093e\u092f\u0924\u093e \u0915\u0947 \u092e\u093e\u0927\u094d\u092f\u092e \u0938\u0947 \u0938\u093e\u092e\u0941\u0926\u093e\u092f\u093f\u0915 \u0938\u094d\u0935\u093e\u0938\u094d\u0925\u094d\u092f \u092a\u0930 \u092a\u094d\u0930\u092d\u093e\u0935 \u0921\u093e\u0932\u0924\u0947 \u0939\u0948\u0902\u0964",
  "ctaDonate": "\u0921\u094b\u0928\u0947\u091f \u0915\u0930\u0947\u0902",
  "ctaProjects": "\u092a\u094d\u0930\u094b\u091c\u0947\u0915\u094d\u091f \u0926\u0947\u0916\u0947\u0902"
